{
  "name": "@dra2020/topojson-server",
  "version": "3.0.102",
  "description": "Convert GeoJSON to TopoJSON for smaller files and the power of topology!",
  "keywords": [
    "topojson",
    "geojson"
  ],
  "homepage": "https://github.com/dra2020/topojson-server",
  "license": "ISC",
  "author": {
    "name": "Mike Bostock",
    "url": "https://bost.ocks.org/mike"
  },
  "main": "dist/@dra2020/topojson-server.js",
  "types": "src/index.d.ts",
  "unpkg": "dist/@dra2020/topojson-server.min.js",
  "jsdelivr": "dist/@dra2020/topojson-server.min.js",
  "module": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dra2020/topojson-server.git"
  },
  "bin": {
    "geo2topo": "bin/geo2topo"
  },
  "files": [
    "bin/geo*",
    "dist/**/*.js",
    "src/**/*.js",
    "src/**/*.ts"
  ],
  "scripts": {
    "pretest": "rollup -c",
    "test": "tape 'test/**/*-test.js' && eslint src test"
  },
  "dependencies": {
    "commander": "2"
  },
  "devDependencies": {
    "eslint": "6",
    "rollup": "1",
    "rollup-plugin-terser": "5",
    "tape": "4",
    "topojson-client": "3"
  }
}
